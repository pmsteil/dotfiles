launchctl load /usr/local/Cellar/nginx/1.19.3/homebrew.mxcl.nginx.plist

echo "nginx started successfully..."
echo ""
echo "Test here:"
echo "   curl http://127.0.0.1:8080  "
echo ""
echo "Important locations:"
echo ""
echo "  Add configs in -> /usr/local/etc/nginx/servers/"
echo "  Default config -> /usr/local/etc/nginx/nginx.conf"
echo "  Logs will be in -> /usr/local/var/log/nginx/"
echo "  Default webroot is -> /usr/local/var/www/"
echo "  Default listen address -> http://localhost:8080"
echo ""
echo ""
